{
    "name": "slack-utility",
    "version": "0.1.24",
    "description": "Set of slack helper functions",
    "main": "dist/index",
    "typings": "dist/index",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npx awesome-readme && npm run lint:fix && tsc --project tsconfig.json",
        "lint:fix": "prettier --write src/**/*.{js,ts} && tslint --fix --config tslint.json --project tsconfig.json",
        "lint": "tslint --config tslint.json --project tsconfig.json",
        "watch": "tsc -w"
    },
    "files": [
        "/dist/**/*.{js,ts,map}",
        "/src/**/*.{js,ts}"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marc-aurele-besner/slack-utility.git"
    },
    "keywords": [
        "slack",
        "slack-api",
        "helper",
        "utility"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marc-aurele-besner/slack-utility/issues"
    },
    "homepage": "https://github.com/marc-aurele-besner/slack-utility#readme",
    "dependencies": {
        "@slack/web-api": "^6.8.1",
        "@types/node": "^18.11.19",
        "ethers": "^5.7.2",
        "faunadb-utility": "^0.1.0",
        "mongoose": "^6.9.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/uuid": "^9.0.0",
        "awesome-readme": "^0.0.8",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typescript": "^4.9.5"
    }
}
